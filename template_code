#ボールのx座標　ボールのy座標　紙コップx座標 紙コップy座標
position_base =[[0.0,0.0,0.0,0.0]]
def position_manager(master_judge,paper_cup,x,y):

    if master == True:
        if len(position_base)>=0:
        
         #ホンスワン
         if paper_cup==True:
             #position_baseの末尾の紙コップのx座標y座標
             return position_base[]
         else:
             #position_baseの末尾のボールのx座標y座標
             return position_base[]
         
     else:
        return position_base[0]
     
        
    else:
    
        #熊谷さん
        #position_base配列の末尾にx,yを追加
        return position_base[] 
    
aa =  position_manager(True,True,0,0)
printf(aa)
